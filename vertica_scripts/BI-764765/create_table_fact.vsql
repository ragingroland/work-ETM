comment on column DataPrime.web_analytics.ETMCliID is 'ID клиента ЭТМ';
comment on column DataPrime.web_analytics.CountryID is 'ID страны';
comment on column DataPrime.web_analytics.RegionID is 'ID региона';
comment on column DataPrime.web_analytics.CityID is 'ID города';
comment on column DataPrime.web_analytics.Device is 'Девайс';
comment on column DataPrime.web_analytics.TrafficSourceFL_first is 'Источник трафика (Перв.)';
comment on column DataPrime.web_analytics.TrafficSourceFL_auto is 'Источник трафика (автоматический.)';
comment on column DataPrime.web_analytics.TrafficSourceFL_last is 'Источник трафика (Посл. Зн.)';
comment on column DataPrime.web_analytics.TypeClient is 'Тип клиента';
