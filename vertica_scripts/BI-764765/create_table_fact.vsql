create table DataPrime.nomenklature (
    CounterID int,
    RgdCode int,
    DataInfo date,
    QltVisit int,
    TrafficSourceID int,
    SearchEngineRootID int,
    WatchCard int,
    QltAdd int,
    SumAdd int,
    QltClickReplace int,
    SumClickReplace int,
    QltPurchaseProd int,
    SumPurchaseProd int,
    QltRefusal int,
    RenomeYM int)
order by
    CounterID,
    TrafficSourceID,
    SearchEngineRootID,
    RgdCode,
    DataInfo
segmented by hash (
    DataPrime.nomenklature.CounterID,
    DataPrime.nomenklature.TrafficSourceID,
    DataPrime.nomenklature.SearchEngineRootID,
    DataPrime.nomenklature.RgdCode) all nodes ksafe;

comment on table DataPrime.nomenklature is 'Номенклатурная выгрузка';
comment on column DataPrime.nomenklature.CounterID is 'ID счетчика';
comment on column DataPrime.nomenklature.RgdCode is 'Код товара';
comment on column DataPrime.nomenklature.DataInfo is 'Дата';
comment on column DataPrime.nomenklature.QltVisit is 'Кол-во визитов с КТ';
comment on column DataPrime.nomenklature.TrafficSourceID is 'Источник траффика';
comment on column DataPrime.nomenklature.SearchEngineRootID is 'Поисковая система';
comment on column DataPrime.nomenklature.WatchCard is 'Кол-во просмотров карточки';
comment on column DataPrime.nomenklature.QltAdd is 'Кол-во добавлений в корзину';
comment on column DataPrime.nomenklature.SumAdd is 'Кол-во добавлений в корзину (шт)';
comment on column DataPrime.nomenklature.QltClickReplace is 'Клик на подобрать замену';
comment on column DataPrime.nomenklature.SumClickReplace is 'Клик на подобрать замену (шт.)';
comment on column DataPrime.nomenklature.QltPurchaseProd is 'Кол-во купленного товара';
comment on column DataPrime.nomenklature.SumPurchaseProd is 'Кол-во купленного товара (шт.)';
comment on column DataPrime.nomenklature.QltRefusal is 'Кол-во отказов';
comment on column DataPrime.nomenklature.RenomeYM is 'Фиксация изменения названия на ЯМ (true/false) если по коду товара (колонка 2 - RgdCode) на предыдущую дату (от даты в колонке 3) произошло изменение названия ЯМ, то фиксируем 1 (true), в ином случае 0 (false)';

